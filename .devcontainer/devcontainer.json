{
  "name": "PDF Table Extraction",
  "image": "python:3.11",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": false,
      "installOhMyZsh": false,
      "upgradePackages": false,
      "username": "vscode",
      "userUid": "1000",
      "userGid": "1000"
    }
  },
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ReprEng.csv"
      ]
    }
  },
  "onCreateCommand": "bash .devcontainer/on-create-command.sh"
}